{
  "build": {
    "content": [
      {
        "files": [
          "_content/**/*.md",
          "_content/**/toc.yml",
          "_content/doc.fx/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "_content/**/*.assets/*",
          "_content/_doc.fx/index.html", 
          "_content/_doc.fx/.nojekyll"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
      , "doc/_doc.fx/templates/docfx-material/material"
      , "doc/_doc.fx/templates/msPro"
    ],
    "globalMetadata": {
      "_enableSearch": "true"
    },
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "keepFileLink": false,
    "noLangKeyword": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
