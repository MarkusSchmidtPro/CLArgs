<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <AssemblyName>MSPro.CLArgs</AssemblyName>
      <RootNamespace>MSPro.CLArgs</RootNamespace>
      <Authors>Markus Schmidt (Pro)</Authors>
      <Company>Markus Schmidt (Pro)</Company>
      <Product>Command Line Arguments</Product>
      <Copyright>(c) copyright 2020, Markus Schmidt (Pro)</Copyright>
      <!--  -->
      <Deterministic>false</Deterministic>
      <AssemblyVersion>1.0.*</AssemblyVersion>
      <Version>1.2009.08.01</Version>
      <FileVersion>$(Version)</FileVersion>
      <Description>Console application command-line support.</Description>
      <PackageIcon></PackageIcon>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <PackageVersion>$(Version)</PackageVersion>
      <Title>CLArgs - Command Line at it's best</Title>
      <PackageProjectUrl>https://github.com/msc4266/CLArgs</PackageProjectUrl>
      <RepositoryUrl>https://github.com/msc4266/CLArgs</RepositoryUrl>
      <RepositoryType>GitHub</RepositoryType>
      <PackageTags>C#, Console , Command Line</PackageTags>
      <AssemblyOriginatorKeyFile>Schmidt_e-Services.snk</AssemblyOriginatorKeyFile>
      <TargetFrameworks>net452;netstandard2.0;netstandard2.1</TargetFrameworks>
      <LangVersion>default</LangVersion>
      <ApplicationIcon></ApplicationIcon>
      <PackageLicenseExpression></PackageLicenseExpression>
      <PackageReleaseNotes>Help support added, fixes null Descriptions</PackageReleaseNotes>
    </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>MSPro.CLArgs.xml</DocumentationFile>
  </PropertyGroup>

  
  <ItemGroup>
    <None Remove="CLArgs.png" />
    <None Remove="command-line.ico" />
    <None Remove="command-line.png" />
    <None Remove="Images\CLArgs.ico" />
    <None Remove="Images\CLArgs.png" />
    <None Remove="Schmidt_e-Services.snk" />
    <None Remove="MSPro.CLArgs.csproj.DotSettings" />
    <None Remove="MSPro.CLArgs.xml" />
    <None Remove="pack.bat" />
    <None Remove="push.bat" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2020.1.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Images\CLArgs.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

</Project>
